<Application x:Class="EvadeWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EvadeWPF"
             xmlns:helpers="clr-namespace:EvadeWPF.Helpers"
             StartupUri="GameWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="GameWindowResources.xaml"/>
             </ResourceDictionary.MergedDictionaries>
             <helpers:ContainerLocator x:Key="ContainerLocator"/>
         </ResourceDictionary>
    </Application.Resources>
</Application>
